@page
@model Parking.Pages.PrkDrive.DeleteModel

@{
    ViewData["Title"] = "Delete";
}
<h3>Movimentação - Exclusão</h3>
<h3>Tem certeza qe deseja excluir este item?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Placa/Veículo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParkingDrive.LicensePlate)
        </dd>
        <dt class="col-sm-2">
            Entrada
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParkingDrive.ParkedAt)
        </dd>
        <dt class="col-sm-2">
            Saída
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ParkingDrive.DisplacedAt)
        </dd>

    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="ParkingDrive.Id" />
        <input type="submit" value="Excluir" class="btn btn-danger" /> |
        <a asp-page="./Index">Voltar</a>
    </form>
</div>
